{
  "name": "History Trimer",
  "description": "It will remove the specified domain from your browser's search history so that it does not remain in your browser's search history",
  "version": "1.2",
  "manifest_version": 3,
  "permissions": ["storage", "history", "browsingData"],
  "background": {
    "service_worker": "./src/background.js",
    "type": "module"
  },
  "options_page": "./pages/options.html",
  "action": {
    "default_icon": "assets/icon-16x16.png"
  },
  "icons": {
    "16": "assets/icon-16x16.png",
    "48": "assets/icon-48x48.png",
    "128": "assets/icon-128x128.png"
  }
}

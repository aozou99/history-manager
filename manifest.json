{
  "name": "History Trimer",
  "description": "It will remove the specified domain from your browser's search history so that it does not remain in your browser's search history",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "storage", "history"],
  "background": {
    "service_worker": "./src/background.js",
    "type": "module"
  },
  "options_page": "./pages/options.html",
  "action": {
    "default_icon": "assets/icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+H"
      },
      "description": "Run extension."
    }
  }
}
